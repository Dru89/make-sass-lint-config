  "author": "Ben Rothman",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sasstools/make-sass-lint-config/issues"
  },
  "homepage": "http://sasstools.github.io/make-sass-lint-config/",
  "dependencies": {
    "commander": "^2.8.1",
    "js-yaml": "^3.4.3"
  },
  "devDependencies": {
    "eslint": "^1.6.0",
    "fs-extra": "^0.24.0",
    "mocha": "^2.3.3"
  }
}
